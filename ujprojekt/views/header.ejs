<nav class="navbar navbar-expand-lg navbar-light bg-light" >

    <div class="container-fluid justify-content-center">
        <a class="navbar-brand"><img class="navbar-brand" src="Kepek/logo.png"></a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
		<div class="collapse navbar-collapse" id="navbarText">
			<ul class="navbar-nav">
			<li class="nav-item"><a class="nav-link" href="/" ><i class="fas fa-user"></i>Kezdőlap</a></li>
			<li class="nav-item"><a class="nav-link" href="/login" ><i class="fas fa-user"></i>Profil</a></li>
			<%if (authUser===undefined){ %>
				<li class="nav-item"><a class="nav-link" href="/kereses" ><i class="fas fa-user"></i>Éttermek</a></li>
			<%}
			else{%>
