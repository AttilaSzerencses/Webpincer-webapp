					<li class="nav-item"><a class="nav-link" href="/admin" ><i class="fas fa-user"></i>Admin</a></li>
				<%}%>
				<%if (authUser.permission==="c" || authUser.permission==="a"){%>
					<li class="nav-item"><a class="nav-link" href="/courier" ><i class="fas fa-user"></i>Rendelések</a></li>
				<%}%>
